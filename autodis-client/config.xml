<?xml version='1.0' encoding='utf-8'?>
<widget id="net.sleepypenguin.autodis_client" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AutoDis Client</name>
    <description>
        A sample feathers.js chat program that uses AutoDis server autodiscovery.
    </description>
    <author email="doughty@sleepypenguin.net" href="http://sleepypenguin.net">
        David Doughty
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-networkinterface" spec="^1.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
